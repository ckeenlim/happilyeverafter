
<div class="container">
	<h4> <%= link_to "Back to home page", root_path %> 
	</h4>

	<h1> Vendors </h1>

	<div>
		<%= form_tag import_vendors_path, multipart: true do %>
	    <%= file_field_tag :file %>
	    <%= submit_tag "Import CSV" %>

	  <% end %>
	 
		
	</div>


	<table class = "table table-striped">
	  <thead>
	    <tr>
	    	<th>Category </th>
	      <th>Id</th>
	      <th>Name</th>
	      <th>Address</th>

	    </tr>
	  </thead>
	  <tbody>
	  	<% if @vendors.nil? %>
	  		<h1> no vendors yet </h1>
	  	<% else %>
		    <% @vendors.each do |vendor| %>
		      <tr>
		      	<td><%= vendor.categoryId %></td>
		        <td><%= vendor.id %></td>
		        <td><%= vendor.name %></td>
		        <td><%= vendor.address %></td>
		      </tr>
		    <% end %>
		  <% end %>
	  </tbody>
	</table>
	
<div>


