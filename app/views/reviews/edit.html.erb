<div class="container">
<h1> Edit </h1>

<%= simple_form_for([@vendor, @review]) do |f| %>
	<div id="rating-form">
		<label> Rating </label>
	</div>
	<%= f.input :rating %>
	<%= f.input :comment %>
	<%= f.button :submit %>
<% end %>

</div>


<script>
	$('#rating-form').raty({
		path: '/assets/',
		scoreName: 'review[rating]'
		score: <%= @review.rating %>
		
	});

</script>